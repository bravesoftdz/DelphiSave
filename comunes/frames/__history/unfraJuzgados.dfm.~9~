  object sdqJuzgado: TSDQuery
    DatabaseName = 'dbprincipal'
    Options = []
    DataSource = fraJuzgado.dsDatos
    SQL.Strings = (
      'SELECT ljz.jz_direccion,'
      '       ljz.jz_localidad,'
      '       ljz.jz_cp,'
      '       ljz.jz_telefono,'
      '       ljz.jz_fax,'
      '       ljz.jz_email'
      '  FROM legales.ljz_juzgado ljz'
      ' WHERE ljz.jz_id = :idjuzgado')
    UniDirectional = True
    Left = 92
    Top = 316
    ParamData = <
      item
        DataType = ftInteger
        Name = 'idjuzgado'
        ParamType = ptInput
      end>
  end
