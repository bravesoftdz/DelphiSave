  object sdqPCT_CANCERTRABAJADOR: TSDQuery
    DatabaseName = 'dbPrincipal'
    Options = []
    SQL.Strings = (
      'SELECT pct.*,'
      '       DECODE(ct_unidadsector,'
      '              '#39'D'#39', '#39'D'#237'as'#39','
      '              '#39'M'#39', '#39'Meses'#39','
      '              '#39'A'#39', '#39'A'#241'os'#39') unidadsector,'
      '       DECODE(ct_unidadempresa,'
      '              '#39'D'#39', '#39'D'#237'as'#39','
      '              '#39'M'#39', '#39'Meses'#39','
      '              '#39'A'#39', '#39'A'#241'os'#39') unidadempresa,'
      '       ce_descripcion, tj_cuil, tj_nombre,'
      '       DECODE(ct_fechabaja, NULL, NULL, '#39'S'#39') baja'
      
        '  FROM hys.pct_cantrabajador pct, hys.pce_cansector, comunes.ctj' +
        '_trabajador'
      ' WHERE ce_id = ct_idsector'
      '   AND tj_id = ct_idtrabajador'
      '   AND ct_idcaninforme = :ci_id')
    Left = 636
    Top = 267
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ci_id'
        ParamType = ptInput
