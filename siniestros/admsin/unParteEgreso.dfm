  inherited sdqDatos: TSDQuery
    AfterClose = sdqDatosAfterClose
    OnNewRecord = sdqDatosNewRecord
    SQL.Strings = (
      
        'SELECT PE_ID, PE_IDEXPEDIENTE, PE_IDPRESTADOR, PE_IDTIPODEEGRESO' +
        ', PE_IDGRAVEDAD,'
      '       NVL(PE_DIAGNOSTICO, PI_DIAGNOSTICO) as PE_DIAGNOSTICO,'
      '       NVL(PE_IDDIAGNOSTICO, DG_ID) as PE_IDDIAGNOSTICO,'
      '       PE_PRESUPINCAPACIDAD, PE_OBSERVACIONES,'
      '       PE_TRATAMIENTO, PE_TRATAMINSTITUCION, PE_TRATAMUBICACION,'
      '       PE_INTERNADODESDE, PE_INTERNADOHASTA, PE_INTERNADODIAS,'
      
        '       NVL(PE_FECHABAJAMEDICA, PI_FECHABAJAMEDICA) as PE_FECHABA' +
        'JAMEDICA,'
      '       PE_FECHAALTALABORAL, PE_FECHAALTAMEDICA,'
      '       PE_FECHAREINICIO, PE_DIASBAJATOTALES, PE_FECHARECONSULTA,'
      
        '       PE_HISTORIACLINICA, PE_MEDICO, PE_RECALIFICACION, PE_FECH' +
        'ARECEPCION,'
      
        '       PE_USUALTA, PE_FECHAALTA, PE_USUMODIF, PE_FECHAMODIF, PE_' +
        'IDTIPOTRATAMIENTO,'
      
        '       PE_FCONFORME, PE_OBRASOCIAL, PE_IDGRAVEDAD_CIE10, PE_IDMO' +
        'TIVONOCARGA,'
      
        '       PE_FECHACARGA, PE_CIE10REVISADO, PE_USUARIOREVISION, PE_F' +
        'ECHAREVISION,'
      
        '       PE_CPAPEL, SE_SECTOR, NVL(EX_FECHARECAIDA, EX_FECHAACCIDE' +
        'NTE) as FACCREC, '
      
        '       EX_FECHAALTA, EX_FECHAACCIDENTE, EX_TIPO, EX_FECHARECEPCI' +
        'ON,'
      
        '       CAST(NULL AS VARCHAR2(1)) AS DA_OPCION, TO_NUMBER(NULL) A' +
        'S DA_HOJAS,'
      
        '       DECODE(PE_INTERNADODESDE, NULL, '#39'N'#39', '#39'S'#39') AS PE_INTERNADO' +
        ','
      
        '       PE_PRESTADOR_NOMBRE, PE_PRESTADOR_TELEFONOS, PE_PRESTADOR' +
        '_CALLE,'
      
        '       PE_PRESTADOR_NUMERO, PE_PRESTADOR_PISO, PE_PRESTADOR_DEPA' +
        'RTAMENTO,'
      
        '       PE_PRESTADOR_LOCALIDAD, PE_PRESTADOR_CPOSTAL, PE_PRESTADO' +
        'R_CPOSTALA,'
      
        '       PE_PRESTADOR_PROVINCIA, EX_FECHACESEILT, PE_MEDICOAUDITOR' +
        ','
      
        '       CAST(NULL AS VARCHAR2(1)) AS DA_OPCIONCI, TO_NUMBER(NULL)' +
        ' AS DA_HOJASCI,'
      
        '       TO_DATE(NULL) AS PV_FECHACONTROL, CAST(NULL AS VARCHAR2(4' +
        '000)) AS PV_OBSERVACIONES,'
      
        '       PE_ABANDONOMIXTO, PE_FECHAABMIXTO, ART.SINIESTRO.GETBAJAM' +
        'EDICA(EX_ID) BAJMED,'
      '       PE_PSIQUIATRIA,'
      
        '       DECODE(PE_CONTPA_ODONTOL, NULL, '#39'N'#39', PE_CONTPA_ODONTOL) A' +
        'S PE_CONTPA_ODONTOL, PE_ALTAODONTDEF,'
      
        '       DECODE(PE_CONTPA_DERMAT, NULL, '#39'N'#39', PE_CONTPA_DERMAT) AS ' +
        'PE_CONTPA_DERMAT, PE_ALTADERMATDEF,'
      
