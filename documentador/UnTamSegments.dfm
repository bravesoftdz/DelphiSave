  object Sdq_Objetos: TSDQuery
    DatabaseName = 'basededatos'
    SQL.Strings = (
      
        'SELECT OBJECT_NAME, SEGMENT_TYPE, CREATED, ROUND((BYTES / 1024 /' +
        ' 1024) ,2)'
      'FROM SYS.DBA_SEGMENTS, SYS.ALL_OBJECTS'
      'WHERE SEGMENT_NAME = OBJECT_NAME'
      'AND SYS.DBA_SEGMENTS.OWNER = :pOwner'
      'AND SEGMENT_TYPE= :pTipo'
      'AND SYS.DBA_SEGMENTS.OWNER = SYS.ALL_OBJECTS.OWNER'
      'ORDER BY BYTES'
      '')
    Left = 16
    Top = 72
    ParamData = <
      item
        DataType = ftString
        Name = 'pOwner'
        ParamType = ptInput
      end
      item
        DataType = ftString
        Name = 'pTipo'
