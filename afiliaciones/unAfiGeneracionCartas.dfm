  object sdqDatos: TSDQuery
    DatabaseName = 'dbprincipal'
    Options = []
    SQL.Strings = (
      
        'SELECT CA_ID, CA_IDTEXTOCARTA, CA_CORREO, CA_TIPOSALIDA, CA_IDFI' +
        'RMA, CA_OBSERVACIONES,'
      
        '       CA_IDFORMULARIO, CA_IDENDOSO, CA_MANUAL, CA_TEXTOFINAL, C' +
        'A_IDDELEGACION,'
      
        '       CA_USUALTA, CA_FECHAALTA, CA_USUMODIF, CA_FECHAMODIF, TC_' +
        'TEXTO,'
      
        '       CA_IDFIRMAREIMPRESION, CA_CORREOREIMPRESION, CA_IDAREATEX' +
        'TO,'
      '       CA_FECHAIMPRESION, CA_IDDELEGACION, CA_IDENDOSOTEMP'
      'FROM CCA_CARTA, CTC_TEXTOCARTA'
      'WHERE CA_IDTEXTOCARTA = TC_ID'
      'AND CA_ID = :pCA_ID'
      'UNION ALL'
      
        'SELECT CA_ID, CA_IDTEXTOCARTA, CA_CORREO, CA_TIPOSALIDA, CA_IDFI' +
        'RMA, CA_OBSERVACIONES,'
      
        '       CA_IDFORMULARIO, CA_IDENDOSO, CA_MANUAL, CA_TEXTOFINAL, C' +
        'A_IDDELEGACION,'
      
        '       CA_USUALTA, CA_FECHAALTA, CA_USUMODIF, CA_FECHAMODIF, TC_' +
        'TEXTO,'
      
        '       CA_IDFIRMAREIMPRESION, CA_CORREOREIMPRESION, CA_IDAREATEX' +
        'TO,'
      '       CA_FECHAIMPRESION, CA_IDDELEGACION, CA_IDENDOSOTEMP'
      'FROM CCA_CARTA, CTC_TEXTOCARTA'
      'WHERE CA_IDTEXTOCARTA = TC_ID'
      'AND CA_ID = :pCA_ID')
    UpdateObject = sduDatos
    Left = 16
    Top = 296
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'pCA_ID'
        ParamType = ptInput
      end
