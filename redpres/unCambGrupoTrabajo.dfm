  object sdqDatos: TSDQuery
    DatabaseName = 'dbPrincipal'
    Options = []
    AfterScroll = sdqDatosAfterScroll
    SQL.Strings = (
      
        'SELECT EX_ID, EX_SINIESTRO, EX_ORDEN, EX_RECAIDA,EX_ESTADO, nvl(' +
        'ex_causafin,'#39#39') ERRORCARGA,'
      
        '               UTILES.ARMAR_SINIESTRO(EX_SINIESTRO, EX_ORDEN, EX' +
        '_RECAIDA ,'#39'-'#39') SINIESTRO,  '
      
        '               ex_cuit, em_nombre, ex_cuil, tj_nombre, ex_fechaa' +
        'ccidente, ex_fecharecaida, amebpba.get_causafin(EX_CAUSAFIN) est' +
        'ado,'
      
        '               ex_diagnostico, ex_diagnosticooms, ex_gtrabajo, e' +
        'x_delegacion, GP_NOMBRE, GP_FBAJA, TB_DESCRIPCION CAUSAFIN, 1.2 ' +
        'dias, siniestro.get_usuariogestor(ex_id) USUGESTOR'
      
        'FROM sex_expedientes, cmp_empresas, ctj_trabajadores, mgp_gtraba' +
        'jo, ctb_tablas'
      'WHERE ex_cuit = mp_cuit'
      'AND ex_cuil = tj_cuil'
      'AND ex_gtrabajo = gp_codigo (+)'
      'AND tb_clave = '#39'CATER'#39
      'AND tb_codigo = ex_causafin'
      ''
      'AND 1=2')
    Left = 20
    Top = 108
