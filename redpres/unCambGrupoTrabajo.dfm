  object sdqGTrabajo: TSDQuery
    DatabaseName = 'dbPrincipal'
    Options = []
    SQL.Strings = (
      'SELECT   gp_codigo, gp_nombre'
      '    FROM art.mgp_gtrabajo, art.mug_usuariogrupotrabajo'
      '   WHERE gp_fbaja IS NULL'
      '     AND gp_siniestros = '#39'S'#39
      '     AND ug_grupo = gp_codigo'
      '     AND ug_usuario = :pusu'
      '     AND (gp_diasmax IS NULL'
      '      OR (gp_diasmax IS NOT NULL AND gp_diasmax > :pdias))'
      '     AND ug_fechabaja IS NULL'
      'ORDER BY gp_nombre')
    Left = 144
    Top = 76
    ParamData = <
      item
        DataType = ftString
        Name = 'pusu'
        ParamType = ptInput
        Value = #39'rsouto'#39
      end
      item
        DataType = ftString
        Name = 'pdias'
        ParamType = ptInput
        Value = '10'
      end>
  end
