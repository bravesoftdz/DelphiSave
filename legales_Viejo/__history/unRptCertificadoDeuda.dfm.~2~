  object sdqEvento: TSDQuery
    DatabaseName = 'dbPrincipal'
    Options = []
    CachedUpdates = False
    SQL.Strings = (
      
        'SELECT   periodo, fija_variable, empleados, LTRIM(TO_CHAR(masa, ' +
        #39'L999G999G990D00'#39', '#39'NLS_NUMERIC_CHARACTERS = '#39#39',.'#39#39' '#39')) masa,'
      
        '         LTRIM(TO_CHAR(devengados, '#39'L999G999G990D00'#39', '#39'NLS_NUMER' +
        'IC_CHARACTERS = '#39#39',.'#39#39' '#39')) devengados,'
      
        '         LTRIM(TO_CHAR(pagos, '#39'L999G999G990D00'#39', '#39'NLS_NUMERIC_CH' +
        'ARACTERS = '#39#39',.'#39#39' '#39')) pagos,'
      
        '         LTRIM(TO_CHAR(devengados - pagos - saldointereses, '#39'L99' +
        '9G999G990D00'#39','
      
        '                       '#39'NLS_NUMERIC_CHARACTERS = '#39#39',.'#39#39' '#39')) sald' +
        'o_nominal,'
      '         DECODE(periodo, ddjj, '#39'SI'#39', '#39'NO'#39') ddjjcalculada,'
      
        '         LTRIM(TO_CHAR(ROUND(deuda *(DECODE(importereclamoafip, ' +
        '0, tasainteres, GREATEST(tasainteres, 0)) / 100), 2)'
      '                       + saldointereses,'
      
        '                       '#39'L999G999G990D00'#39', '#39'NLS_NUMERIC_CHARACTER' +
        'S = '#39#39',.'#39#39' '#39')) interes,'
      '         LTRIM'
      
        '              (TO_CHAR(ROUND(TO_NUMBER(art.utiles.iif_compara('#39'<' +
        #39', periodo, '#39'199606'#39', 0,'
      
        '                                                              de' +
        'uda'
      
        '                                                              + ' +
        'ROUND(deuda'
      
        '                                                                ' +
        '      *(DECODE(importereclamoafip,'
      
        '                                                                ' +
        '               0, tasainteres,'
      
        '                                                                ' +
        '               GREATEST(tasainteres, 0))'
      
        '                                                                ' +
        '        / 100),'
      
        '                                                                ' +
        '      2))),'
      '                             2)'
      '                       + saldointereses,'
      
        '                       '#39'L999G999G990D00'#39', '#39'NLS_NUMERIC_CHARACTER' +
        'S = '#39#39',.'#39#39' '#39')) saldo_consolidada'
      '    FROM (SELECT rc_periodo periodo, rc_contrato contrato,'
      
        '                 art.afiliacion.get_tarifavigentetexto(rc_contra' +
        'to, rc_periodo) fija_variable, rc_periodonomina ddjj,'
      
        '                 rc_empleados empleados, rc_masasalarial masa, r' +
        'c_importereclamado importereclamoafip,'
      
        '                 rc_devengadocuota + rc_devengadofondo + rc_deve' +
        'ngadointeres + rc_devengadootros devengados,'
      
        '                 rc_pagocuota + rc_pagofondo + rc_pagointeres + ' +
        'rc_pagootros + rc_recuperocuota + rc_recuperofondo'
      '                 + rc_recuperointeres pagos,'
      
        '                 (rc_devengadocuota + rc_devengadofondo + rc_dev' +
        'engadointeres + rc_devengadootros)'
      
        '                 -(rc_pagocuota + rc_pagofondo + rc_pagointeres ' +
        '+ rc_pagootros + rc_recuperocuota + rc_recuperofondo'
      '                   + rc_recuperointeres)'
      
        '                 - rc_importereclamado - rc_montorefinanciado - ' +
        'art.cobranza.getsaldointereses(rc_contrato, rc_periodo) deuda,'
      
        '                 rc_montorefinanciado montorefinanciado, art.cob' +
        'ranza.getsaldointereses(rc_contrato, rc_periodo) saldointereses,'
      
        '                 art.deuda.get_tasaacumulada(art.deuda.get_venci' +
        'mientocuota(SUBSTR(:cuit, 11, 1), rc_periodo,  art.afiliacion.ge' +
        't_idregimencontrato(rc_contrato) ) + 1,'
      
        '                                             :fechainteres) tasa' +
        'interes,'
      
        '                 DECODE('#39#39', NULL, '#39'N'#39', art.utiles.iif_compara('#39'<' +
        '='#39', rc_periodo, '#39#39', '#39'S'#39', '#39'N'#39')) esconcursoquiebra,'
      '                 0 extrajudicial, rc_prescripto prescripto'
      '            FROM zrc_resumencobranza'
      '           WHERE rc_contrato = :contrato'
      '             AND rc_periodo >= :perdesde'
      '             AND rc_periodo <= :perhasta)'
      '   WHERE devengados - pagos - importereclamoafip > 0'
      
        '     AND art.cobranza.is_nomostrarreclamoafip(devengados, pagos,' +
        ' importereclamoafip, periodo) = '#39'S'#39
      '     AND esconcursoquiebra <> '#39'S'#39
      '     AND prescripto <> '#39'S'#39
      'ORDER BY 1 DESC')
    Left = 19
    Top = 429
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cuit'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'fechainteres'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'contrato'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'perdesde'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'perhasta'
        ParamType = ptInput
      end>
  end
