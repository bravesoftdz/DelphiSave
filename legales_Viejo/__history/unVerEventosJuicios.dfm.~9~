  inherited sdqConsulta: TSDQuery
    AfterScroll = sdqConsultaAfterScroll
    SQL.Strings = (
      
        'SELECT jt_id, jt_numerocarpeta, jt_fechanotificacionjuicio, jt_i' +
        'dabogado, bo_nombre, et_id, et_fechaevento, et_fechavencimiento,'
      
        '       et_idtipoevento, te_descripcion, et_usualta, et_fechaalta' +
        ', et_observaciones, et_fechabaja, et_usumodif, et_fechamodif,'
      
        '       jt_demandante || '#39' - '#39' || jt_demandado || '#39' - Nro Exp.:'#39' ' +
        '|| jt_nroexpediente || '#39' - Estado:'#39' || ej_descripcion'
      
        '       || '#39' - RH:'#39' || LTRIM(TO_CHAR(jt_importereservahonorarios,' +
        ' '#39'L999G999G990D00'#39', '#39'NLS_NUMERIC_CHARACTERS = '#39#39',.'#39#39#39'))'
      
        '       || '#39' - RC:'#39' || LTRIM(TO_CHAR(jt_importereserva, '#39'L999G999' +
        'G990D00'#39', '#39'NLS_NUMERIC_CHARACTERS = '#39#39',.'#39#39#39'))'
      
        '       || '#39'- $ Dem:'#39' || LTRIM(TO_CHAR(jt_importedemandado, '#39'L999' +
        'G999G990D00'#39', '#39'NLS_NUMERIC_CHARACTERS = '#39#39',.'#39#39#39')) datos'
      
        '  FROM legales.ljt_juicioentramite, legales.let_eventojuicioentr' +
        'amite let, legales.lte_tipoevento, legales.lbo_abogado,'
      '       legales.lej_estadojuicio'
      ' WHERE et_idjuicioentramite = jt_id'
      '   AND et_idtipoevento = te_id'
      '   AND jt_idestado = ej_id'
      '   AND jt_idabogado = bo_id'
      '   AND jt_estadomediacion = '#39'J'#39
      '   AND 1=2')
  end
