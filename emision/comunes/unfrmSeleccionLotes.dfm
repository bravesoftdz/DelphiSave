  object sdqLotes: TSDQuery
    DatabaseName = 'dbPrincipal'
    SQL.Strings = (
      'SELECT ILO.*, '
      
        '(SELECT COUNT(*) FROM EMI.ILD_LOTEDDJJ ILD WHERE ILD.ld_idlote =' +
        ' ILO.LO_ID) AS DDJJ_ASIGNADAS, '
      
        '(SELECT COUNT(*) FROM EMI.IGL_GESTORLOTE IGL WHERE IGL.GL_IDLOTE' +
        ' = ILO.LO_ID) AS GESTORES_ASIGNADOS '
      'FROM EMI.ILO_LOTE  ILO'
      'WHERE LO_FECHABAJA IS NULL'
      '')
    Left = 8
    Top = 272
  end
